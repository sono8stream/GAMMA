{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "ordered-imports": [false,{"grouped-imports": false}],
    "member-ordering": false
  },
  "jsRules": {
    "no-console": false,
    "ordered-imports": [false,{"grouped-imports": false}],
    "member-ordering": false
  }
}
